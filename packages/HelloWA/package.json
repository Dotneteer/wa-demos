{
  "name": "hello-wa",
  "version": "1.0.0",
  "description": "Hello from WebAssembly app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dotneteer/wa-demos.git"
  },
  "scripts": {
    "build": "tsc -p ./src && wat2wasm ./src/hello.wat -o ./build/hello.wasm",
    "open": "http-server -a localhost -p 3131 -o /src",
    "start": "npm run build && npm run open"
  },
  "keywords": [
    "WebAssembly",
    "WA"
  ],
  "author": "Istvan Novak",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^13.0.0",
    "typescript": "^4.3.5",
    "wat2wasm": "^3.0.0"
  }
}
